# Grafana Cloud Credentials
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# =============================================================================
# Grafana Cloud -- Metrics (Prometheus remote write)
# =============================================================================
# Find these at: grafana.com -> My Account -> Prometheus -> Details
GRAFANA_METRICS_URL=https://prometheus-prod-XX-prod-us-east-0.grafana.net/api/prom/push
GRAFANA_METRICS_USERNAME=000000
GRAFANA_METRICS_API_KEY=glc_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# =============================================================================
# Grafana Cloud -- Logs (Loki)
# =============================================================================
# Find these at: grafana.com -> My Account -> Loki -> Details
GRAFANA_LOGS_URL=https://logs-prod-XX.grafana.net/loki/api/v1/push
GRAFANA_LOGS_USERNAME=000000
GRAFANA_LOGS_API_KEY=glc_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# =============================================================================
# Grafana Cloud -- Traces (Tempo) â€” NOT USED by this demo
# =============================================================================
# This demo does not send traces. Uncomment if you add tracing support.
# GRAFANA_TRACES_URL=https://tempo-prod-XX-prod-us-east-0.grafana.net:443
# GRAFANA_TRACES_USERNAME=000000
# GRAFANA_TRACES_API_KEY=glc_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# =============================================================================
# Demo-specific -- Oracle
# =============================================================================
# Oracle DB system password (grafanau and demo_user passwords are hardcoded
# to "oracle" in init.sql)
ORACLE_PASSWORD=oracle
